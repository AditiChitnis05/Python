n = int(input())

if n>0 and n<101:
    # first condition - if number is odd print weird
    if n%2!=0:
        print("Weird")
    
    # second condition - n is even and in range of 2 to 5 print not weird
    elif n%2==0 and n in range(2,6):
        print("Not Weird")
    
    # third condition - n is even and in range 6 to 20 print weird
    elif n%2==0 and n in range(6,21):
        print("Weird")
    
    # fourth condition - n is even and greater than 20 print not weird
    elif n%2==0 and n>20:
        print("Not Weird")
